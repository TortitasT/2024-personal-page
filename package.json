{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "build:preview": "vite build && vite preview",
    "prettier": "prettier --write .",
    "bin:compile": "tsc -p ./bin --outDir ./bin/dist",
    "bin:download-tech": "pnpm run bin:compile && node --enable-source-maps ./bin/dist/download-tech.js",
    "bin:download-projects": "pnpm run bin:compile && node --enable-source-maps ./bin/dist/download-projects.js",
    "bin:download-all": "pnpm run bin:download-tech && pnpm run bin:download-projects"
  },
  "dependencies": {
    "@mdx-js/rollup": "^3.0.0",
    "gsap": "^3.12.2",
    "preact": "^10.16.0",
    "preact-render-to-string": "^6.2.0",
    "remark-frontmatter": "^5.0.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@types/node": "^20.10.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-preact": "^1.3.0",
    "postcss": "^8.4.31",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.3",
    "vike": "^0.4.143",
    "vite": "^4.3.2"
  },
  "eslintConfig": {
    "extends": "preact"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
